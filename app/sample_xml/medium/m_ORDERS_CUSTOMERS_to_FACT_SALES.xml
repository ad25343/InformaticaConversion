      <TARGET BUSINESSNAME="" DATABASETYPE="Oracle"
              NAME="FACT_SALES" OWNERNAME="DWH">
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="10"  NAME="ORDER_ID"         KEYTYPE="PRIMARY KEY" FIELDNUMBER="1"  PRECISION="10" SCALE="0" NULLABLE="NOTNULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="10"  NAME="CUSTOMER_ID"      KEYTYPE="NOT A KEY"   FIELDNUMBER="2"  PRECISION="10" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="100" NAME="CUSTOMER_NAME"    KEYTYPE="NOT A KEY"   FIELDNUMBER="3"  PRECISION="100" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="20"  NAME="CUSTOMER_TIER"    KEYTYPE="NOT A KEY"   FIELDNUMBER="4"  PRECISION="20" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="10"  NAME="PRODUCT_ID"       KEYTYPE="NOT A KEY"   FIELDNUMBER="5"  PRECISION="10" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="DATE"     LENGTH="29"  NAME="ORDER_DATE"       KEYTYPE="NOT A KEY"   FIELDNUMBER="6"  PRECISION="29" SCALE="9" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="7"   NAME="ORDER_YEAR_MONTH" KEYTYPE="NOT A KEY"   FIELDNUMBER="7"  PRECISION="7"  SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="15"  NAME="ORDER_AMOUNT"     KEYTYPE="NOT A KEY"   FIELDNUMBER="8"  PRECISION="15" SCALE="2" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="15"  NAME="DISCOUNT_AMOUNT"  KEYTYPE="NOT A KEY"   FIELDNUMBER="9"  PRECISION="15" SCALE="2" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="15"  NAME="NET_AMOUNT"       KEYTYPE="NOT A KEY"   FIELDNUMBER="10" PRECISION="15" SCALE="2" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="NUMBER"   LENGTH="5"   NAME="QUANTITY"         KEYTYPE="NOT A KEY"   FIELDNUMBER="11" PRECISION="5"  SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="10"  NAME="CHANNEL"          KEYTYPE="NOT A KEY"   FIELDNUMBER="12" PRECISION="10" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="20"  NAME="REGION_CODE"      KEYTYPE="NOT A KEY"   FIELDNUMBER="13" PRECISION="20" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="50"  NAME="REGION_NAME"      KEYTYPE="NOT A KEY"   FIELDNUMBER="14" PRECISION="50" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="20"  NAME="ORDER_STATUS_DESC" KEYTYPE="NOT A KEY"  FIELDNUMBER="15" PRECISION="20" SCALE="0" NULLABLE="NULL"/>
        <TARGETFIELD DATATYPE="VARCHAR2" LENGTH="1"   NAME="IS_LARGE_ORDER"   KEYTYPE="NOT A KEY"   FIELDNUMBER="16" PRECISION="1"  SCALE="0" NULLABLE="NULL"/>
      </TARGET>

      <!-- ═══════════════════════════════════════════════════════
           MAPPING: m_ORDERS_CUSTOMERS_to_FACT_SALES
           Complexity: MEDIUM
           Transformations: SQ_ORDERS, SQ_CUSTOMERS, JNR_ORDERS_CUSTOMERS,
                            LKP_REGION, EXP_CALCULATIONS, FIL_VALID_ORDERS,
                            AGG_DISCOUNT, Target (8 transformations)
           Logic: Join orders to customers, lookup region, apply discount rules,
                  aggregate discounts by tier, filter invalid orders
      ════════════════════════════════════════════════════════ -->
      <MAPPING DESCRIPTION="Join orders and customers, enrich with region lookup, apply discount and filter logic, load to sales fact."
               ISVALID="YES" NAME="m_ORDERS_CUSTOMERS_to_FACT_SALES"
               OBJECTVERSION="1" VERSIONNUMBER="1">

        <!-- SQ: Orders -->
        <TRANSFORMATION NAME="SQ_STG_ORDERS" TYPE="Source Qualifier"
                        REUSABLE="NO" DESCRIPTION="Read orders — exclude CANCELLED status">
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_ID"     PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID"  PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="PRODUCT_ID"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="DATE"     NAME="ORDER_DATE"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="29" SCALE="9"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_AMOUNT" PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="15" SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="QUANTITY"     PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="5"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_STATUS" PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="20" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CHANNEL"      PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10" SCALE="0"/>
          <TABLEATTRIBUTE NAME="Sql Query"     VALUE=""/>
          <TABLEATTRIBUTE NAME="Source Filter" VALUE="ORDER_STATUS != &apos;CANCELLED&apos;"/>
        </TRANSFORMATION>

        <!-- SQ: Customers -->
        <TRANSFORMATION NAME="SQ_STG_CUSTOMERS" TYPE="Source Qualifier"
                        REUSABLE="NO" DESCRIPTION="Read all customers">
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_NAME" PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="100" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="EMAIL"         PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="50"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="COUNTRY_CODE"  PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_TIER" PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="20"  SCALE="0"/>
          <TABLEATTRIBUTE NAME="Sql Query" VALUE=""/>
        </TRANSFORMATION>

        <!-- Joiner: Orders ⋈ Customers on CUSTOMER_ID -->
        <TRANSFORMATION NAME="JNR_ORDERS_CUSTOMERS" TYPE="Joiner"
                        REUSABLE="NO" DESCRIPTION="Inner join orders to customers on CUSTOMER_ID. Orders is detail, Customers is master.">
          <!-- Master (Customers) input ports -->
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID_M"   PORTTYPE="INPUT"  EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_NAME"   PORTTYPE="INPUT"  EXPRESSION="" PRECISION="100" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="EMAIL"           PORTTYPE="INPUT"  EXPRESSION="" PRECISION="50"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="COUNTRY_CODE"    PORTTYPE="INPUT"  EXPRESSION="" PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_TIER"   PORTTYPE="INPUT"  EXPRESSION="" PRECISION="20"  SCALE="0"/>
          <!-- Detail (Orders) input ports -->
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_ID"        PORTTYPE="INPUT"  EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID_D"   PORTTYPE="INPUT"  EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="PRODUCT_ID"      PORTTYPE="INPUT"  EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="DATE"     NAME="ORDER_DATE"      PORTTYPE="INPUT"  EXPRESSION="" PRECISION="29"  SCALE="9"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_AMOUNT"    PORTTYPE="INPUT"  EXPRESSION="" PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="QUANTITY"        PORTTYPE="INPUT"  EXPRESSION="" PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_STATUS"    PORTTYPE="INPUT"  EXPRESSION="" PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CHANNEL"         PORTTYPE="INPUT"  EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <!-- Output ports -->
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="OUT_ORDER_ID"       PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="OUT_CUSTOMER_ID"    PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="OUT_CUSTOMER_NAME"  PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="100" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="OUT_COUNTRY_CODE"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="OUT_CUSTOMER_TIER"  PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="OUT_PRODUCT_ID"     PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="DATE"     NAME="OUT_ORDER_DATE"     PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="29"  SCALE="9"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="OUT_ORDER_AMOUNT"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="OUT_QUANTITY"       PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="OUT_ORDER_STATUS"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="OUT_CHANNEL"        PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="10"  SCALE="0"/>
          <TABLEATTRIBUTE NAME="Join Type"      VALUE="Normal"/>
          <TABLEATTRIBUTE NAME="Join Condition" VALUE="CUSTOMER_ID_M = CUSTOMER_ID_D"/>
          <TABLEATTRIBUTE NAME="Master Driver"  VALUE="STG_CUSTOMERS"/>
        </TRANSFORMATION>

        <!-- Lookup: DIM_REGION on COUNTRY_CODE -->
        <TRANSFORMATION NAME="LKP_DIM_REGION" TYPE="Lookup"
                        REUSABLE="NO" DESCRIPTION="Static lookup to DIM_REGION to resolve COUNTRY_CODE to region details. Returns REGION_CODE, REGION_NAME, SUB_REGION. Default to UNKNOWN if not found.">
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="COUNTRY_CODE"  PORTTYPE="INPUT"  EXPRESSION="" PRECISION="5"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_CODE"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="20" SCALE="0" DEFAULTVALUE="&apos;UNKNOWN&apos;"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_NAME"   PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="50" SCALE="0" DEFAULTVALUE="&apos;UNKNOWN&apos;"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="SUB_REGION"    PORTTYPE="OUTPUT" EXPRESSION="" PRECISION="30" SCALE="0" DEFAULTVALUE="&apos;UNKNOWN&apos;"/>
          <TABLEATTRIBUTE NAME="Lookup Table"     VALUE="DIM_REGION"/>
          <TABLEATTRIBUTE NAME="Lookup Condition" VALUE="COUNTRY_CODE = COUNTRY_CODE"/>
          <TABLEATTRIBUTE NAME="Lookup Policy on Multiple Match" VALUE="Return First"/>
          <TABLEATTRIBUTE NAME="Lookup Cache Persistent" VALUE="NO"/>
        </TRANSFORMATION>

        <!-- Expression: business calculations -->
        <TRANSFORMATION NAME="EXP_CALCULATIONS" TYPE="Expression"
                        REUSABLE="NO" DESCRIPTION="Calculate discount by customer tier, derive net amount, format dates, derive status description and large order flag">
          <!-- Pass-through inputs -->
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_ID"       PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_ORDER_ID"      PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID"    PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_CUSTOMER_ID"   PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_NAME"  PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_CUSTOMER_NAME" PRECISION="100" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_TIER"  PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_CUSTOMER_TIER" PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="PRODUCT_ID"     PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_PRODUCT_ID"    PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="DATE"     NAME="ORDER_DATE"     PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_ORDER_DATE"    PRECISION="29"  SCALE="9"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_AMOUNT"   PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_ORDER_AMOUNT"  PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="QUANTITY"       PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_QUANTITY"      PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_STATUS"   PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_ORDER_STATUS"  PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CHANNEL"        PORTTYPE="INPUT/OUTPUT" EXPRESSION="OUT_CHANNEL"       PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_CODE"    PORTTYPE="INPUT/OUTPUT" EXPRESSION="REGION_CODE"       PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_NAME"    PORTTYPE="INPUT/OUTPUT" EXPRESSION="REGION_NAME"       PRECISION="50"  SCALE="0"/>
          <!-- Derived: discount by tier — GOLD 10%, SILVER 5%, BRONZE 2%, else 0 -->
          <TRANSFORMFIELD DATATYPE="NUMBER" NAME="DISCOUNT_AMOUNT"
                          PORTTYPE="OUTPUT"
                          EXPRESSION="ROUND(ORDER_AMOUNT * DECODE(CUSTOMER_TIER, &apos;GOLD&apos;, 0.10, &apos;SILVER&apos;, 0.05, &apos;BRONZE&apos;, 0.02, 0), 2)"
                          PRECISION="15" SCALE="2"/>
          <!-- Derived: net amount after discount -->
          <TRANSFORMFIELD DATATYPE="NUMBER" NAME="NET_AMOUNT"
                          PORTTYPE="OUTPUT"
                          EXPRESSION="ORDER_AMOUNT - ROUND(ORDER_AMOUNT * DECODE(CUSTOMER_TIER, &apos;GOLD&apos;, 0.10, &apos;SILVER&apos;, 0.05, &apos;BRONZE&apos;, 0.02, 0), 2)"
                          PRECISION="15" SCALE="2"/>
          <!-- Derived: year-month of order -->
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_YEAR_MONTH"
                          PORTTYPE="OUTPUT"
                          EXPRESSION="TO_CHAR(ORDER_DATE, &apos;YYYY-MM&apos;)"
                          PRECISION="7" SCALE="0"/>
          <!-- Derived: human readable status -->
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_STATUS_DESC"
                          PORTTYPE="OUTPUT"
                          EXPRESSION="DECODE(ORDER_STATUS, &apos;COMP&apos;, &apos;COMPLETED&apos;, &apos;PEND&apos;, &apos;PENDING&apos;, &apos;PROC&apos;, &apos;PROCESSING&apos;, &apos;RTRN&apos;, &apos;RETURNED&apos;, &apos;UNKNOWN&apos;)"
                          PRECISION="20" SCALE="0"/>
          <!-- Derived: flag orders over $5000 as large -->
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="IS_LARGE_ORDER"
                          PORTTYPE="OUTPUT"
                          EXPRESSION="IIF(ORDER_AMOUNT &gt; 5000, &apos;Y&apos;, &apos;N&apos;)"
                          PRECISION="1" SCALE="0"/>
        </TRANSFORMATION>

        <!-- Filter: exclude zero-amount and negative orders -->
        <TRANSFORMATION NAME="FIL_VALID_ORDERS" TYPE="Filter"
                        REUSABLE="NO" DESCRIPTION="Exclude orders with zero or negative amounts and null order dates">
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_ID"          PORTTYPE="INPUT/OUTPUT" EXPRESSION="ORDER_ID"          PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="CUSTOMER_ID"       PORTTYPE="INPUT/OUTPUT" EXPRESSION="CUSTOMER_ID"       PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_NAME"     PORTTYPE="INPUT/OUTPUT" EXPRESSION="CUSTOMER_NAME"     PRECISION="100" SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CUSTOMER_TIER"     PORTTYPE="INPUT/OUTPUT" EXPRESSION="CUSTOMER_TIER"     PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="PRODUCT_ID"        PORTTYPE="INPUT/OUTPUT" EXPRESSION="PRODUCT_ID"        PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="DATE"     NAME="ORDER_DATE"        PORTTYPE="INPUT/OUTPUT" EXPRESSION="ORDER_DATE"        PRECISION="29"  SCALE="9"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_YEAR_MONTH"  PORTTYPE="INPUT/OUTPUT" EXPRESSION="ORDER_YEAR_MONTH"  PRECISION="7"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="ORDER_AMOUNT"      PORTTYPE="INPUT/OUTPUT" EXPRESSION="ORDER_AMOUNT"      PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="DISCOUNT_AMOUNT"   PORTTYPE="INPUT/OUTPUT" EXPRESSION="DISCOUNT_AMOUNT"   PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="NET_AMOUNT"        PORTTYPE="INPUT/OUTPUT" EXPRESSION="NET_AMOUNT"        PRECISION="15"  SCALE="2"/>
          <TRANSFORMFIELD DATATYPE="NUMBER"   NAME="QUANTITY"          PORTTYPE="INPUT/OUTPUT" EXPRESSION="QUANTITY"          PRECISION="5"   SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="CHANNEL"           PORTTYPE="INPUT/OUTPUT" EXPRESSION="CHANNEL"           PRECISION="10"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_CODE"       PORTTYPE="INPUT/OUTPUT" EXPRESSION="REGION_CODE"       PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="REGION_NAME"       PORTTYPE="INPUT/OUTPUT" EXPRESSION="REGION_NAME"       PRECISION="50"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="ORDER_STATUS_DESC" PORTTYPE="INPUT/OUTPUT" EXPRESSION="ORDER_STATUS_DESC" PRECISION="20"  SCALE="0"/>
          <TRANSFORMFIELD DATATYPE="VARCHAR2" NAME="IS_LARGE_ORDER"    PORTTYPE="INPUT/OUTPUT" EXPRESSION="IS_LARGE_ORDER"    PRECISION="1"   SCALE="0"/>
          <TABLEATTRIBUTE NAME="Filter Condition" VALUE="ORDER_AMOUNT &gt; 0 AND NOT ISNULL(ORDER_DATE)"/>
        </TRANSFORMATION>

        <!-- Connectors: SQ_ORDERS → JNR (detail) -->
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="ORDER_ID"     TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="ORDER_ID"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="CUSTOMER_ID"  TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="CUSTOMER_ID_D"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="PRODUCT_ID"   TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="PRODUCT_ID"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="ORDER_DATE"   TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="ORDER_DATE"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="ORDER_AMOUNT" TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="ORDER_AMOUNT"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="QUANTITY"     TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="QUANTITY"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="ORDER_STATUS" TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="ORDER_STATUS"/>
        <CONNECTOR FROMINSTANCE="SQ_STG_ORDERS"    FROMINSTANCETYPE="Source Qualifier" FROMFIELD="CHANNEL"      TOINSTANCE="JNR_ORDERS_CUSTOMERS" TOINSTANCETYPE="Joiner" TOFIELD="CHANNEL"/>

        <!-- Connectors: SQ_CUSTOMERS → JNR (master) -->
