        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="5"   NAME="COUNTRY_CODE"   NULLABLE="NOTNULL" FIELDNUMBER="1"  PRECISION="5"  SCALE="0"/>
        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="50"  NAME="REGION_NAME"    NULLABLE="NULL"    FIELDNUMBER="2"  PRECISION="50" SCALE="0"/>
        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="20"  NAME="REGION_CODE"    NULLABLE="NULL"    FIELDNUMBER="3"  PRECISION="20" SCALE="0"/>
        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="30"  NAME="SUB_REGION"     NULLABLE="NULL"    FIELDNUMBER="4"  PRECISION="30" SCALE="0"/>
      </SOURCE>

      <!-- ═══════════════════════════════════════════════════════
