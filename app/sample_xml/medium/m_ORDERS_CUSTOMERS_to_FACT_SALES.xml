        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="100" NAME="CUSTOMER_NAME"  NULLABLE="NULL"    FIELDNUMBER="2"  PRECISION="100" SCALE="0"/>
        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="50"  NAME="EMAIL"          NULLABLE="NULL"    FIELDNUMBER="3"  PRECISION="50" SCALE="0"/>
        <SOURCEFIELD DATATYPE="VARCHAR2" LENGTH="5"   NAME="COUNTRY_CODE"   NULLABLE="NULL"    FIELDNUMBER="4"  PRECISION="5"  SCALE="0"/>
