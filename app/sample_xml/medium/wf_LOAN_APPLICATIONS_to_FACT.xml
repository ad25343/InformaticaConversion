<!DOCTYPE POWERMART SYSTEM "powrmart.dtd">
<!--
  Paired workflow for m_LOAN_APPLICATIONS_to_FACT.
  Upload together with params_LOAN_APPLICATIONS_to_FACT.txt
  to exercise v1.1 multi-file upload and Step 0 session extraction.
-->
<POWERMART CREATION_DATE="02/21/2026 10:00:00" REPOSITORY_VERSION="186.96">
  <REPOSITORY NAME="DEV_REPO" VERSION="186" CODEPAGE="UTF-8" DATABASETYPE="Oracle">
    <FOLDER NAME="LOAN_ETL" GROUP="" OWNER="admin" SHARED="NOTSHARED"
            DESCRIPTION="ETL Workflows">

      <WORKFLOW DESCRIPTION="Workflow for m_LOAN_APPLICATIONS_to_FACT"
                NAME="WF_LOAN_APPLICATIONS_to_FACT"
                REUSABLE="NO"
                SCHEDULERNAME="Scheduler"
                SERVERNAME="DEV_IS"
                ISENABLED="YES">

        <TASK DESCRIPTION="" NAME="Start" REUSABLE="NO" TYPE="Start"/>

        <SESSION DESCRIPTION="Session for m_LOAN_APPLICATIONS_to_FACT"
                 NAME="s_LOAN_APPLICATIONS_to_FACT"
                 MAPPINGNAME="m_LOAN_APPLICATIONS_to_FACT"
                 REUSABLE="NO">

          <CONNECTIONREFERENCE
            CONNECTIONNAME="$$SRC_LOANS_CONNECTION"
            CONNECTIONSUBTYPE="Oracle"
            CONNECTIONTYPE="Relational"
            TRANSFORMATIONINSTANCENAME="SQ_LOAN_APPLICATIONS"
            ROLE="SOURCE"/>
          <CONNECTIONREFERENCE
            CONNECTIONNAME="$$SRC_APPRAISAL_CONNECTION"
            CONNECTIONSUBTYPE="Oracle"
            CONNECTIONTYPE="Relational"
            TRANSFORMATIONINSTANCENAME="SQ_PROPERTY_APPRAISALS"
            ROLE="SOURCE"/>
          <CONNECTIONREFERENCE
            CONNECTIONNAME="$$SRC_LOANS_CONNECTION"
            CONNECTIONSUBTYPE="Oracle"
            CONNECTIONTYPE="Relational"
            TRANSFORMATIONINSTANCENAME="LKP_COUNTY_LOAN_LIMITS"
            ROLE="SOURCE"/>
          <CONNECTIONREFERENCE
            CONNECTIONNAME="$$TGT_CONNECTION"
            CONNECTIONSUBTYPE="Oracle"
            CONNECTIONTYPE="Relational"
            TRANSFORMATIONINSTANCENAME="TGT_FACT_LOAN_APPLICATIONS"
            ROLE="TARGET"/>

          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="s_LOAN_APPLICATIONS_to_FACT"
            ATTRIBUTENAME="Commit Interval"
            ATTRIBUTEVALUE="10000"/>

          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="s_LOAN_APPLICATIONS_to_FACT"
            ATTRIBUTENAME="Stop On Errors"
            ATTRIBUTEVALUE="0"/>

          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="TGT_FACT_LOAN_APPLICATIONS"
            ATTRIBUTENAME="Reject Filename"
            ATTRIBUTEVALUE="LOAN_APPLICATIONS_to_FACT.bad"/>

          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="TGT_FACT_LOAN_APPLICATIONS"
            ATTRIBUTENAME="Reject File Directory"
            ATTRIBUTEVALUE="$$REJECT_FILE_DIR"/>
          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="TGT_FACT_LOAN_APPLICATIONS"
            ATTRIBUTENAME="Pre SQL"
            ATTRIBUTEVALUE="DELETE FROM DWH.FACT_LOAN_APPLICATIONS WHERE LOAD_DATE = TO_DATE('$$LOAD_DATE','YYYY-MM-DD')"/>
          <SESSTRANSFORMINSTATTR
            TRANSFORMATIONINSTANCENAME="TGT_FACT_LOAN_APPLICATIONS"
            ATTRIBUTENAME="Post SQL"
            ATTRIBUTEVALUE="UPDATE ETL_AUDIT SET STATUS='COMPLETE' WHERE JOB='WF_LOAN_APPLICATIONS_to_FACT'"/>

        </SESSION>

        <TASKINSTANCEPATH FROM="Start" TO="s_LOAN_APPLICATIONS_to_FACT"/>

      </WORKFLOW>

    </FOLDER>
  </REPOSITORY>
</POWERMART>
